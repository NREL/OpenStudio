1 RT_MANIFEST "${CMAKE_CURRENT_SOURCE_DIR}/openstudio.exe.manifest"
1 VERSIONINFO
FILEVERSION ${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH_DIGIT}
PRODUCTVERSION ${PROJECT_VERSION_MAJOR},${PROJECT_VERSION_MINOR},${PROJECT_VERSION_PATCH_DIGIT}
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName",      "NREL\0"
            VALUE "FileDescription",  "OpenStudio CLI\0"
            VALUE "FileVersion",      "${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}\0"
            VALUE "InternalName",     "openstudio\0"
            VALUE "LegalCopyright",   "Copyright (c) 2008-2025, Alliance for Sustainable Energy, LLC, and other contributors. All rights reserved.\0"
            VALUE "LegalTrademarks",  "OpenStudio (TM) is a trademark of NREL\0"
            VALUE "OriginalFilename", "openstudio.exe\0"
            VALUE "ProductName",      "OpenStudio\0"
            VALUE "ProductVersion",   "${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}.${PROJECT_VERSION_BUILD}\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
