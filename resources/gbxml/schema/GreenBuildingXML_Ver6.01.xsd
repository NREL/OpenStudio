				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E">
				<xsd:annotation>
					<xsd:documentation>4-in. face brick + 4-in. block</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="F">
				<xsd:annotation>
					<xsd:documentation>4-in. block + air space or insulation</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
