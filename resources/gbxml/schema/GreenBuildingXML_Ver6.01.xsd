	<xsd:element name="Absorptance">
		<xsd:annotation>
			<xsd:documentation>Absorptance of the outside surface</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:decimal">
					<xsd:attribute name="unit" type="unitlessUnitEnum" use="required"/>
